{
  "version": 2,
  "buildCommand": "echo 'Static site deployment'",
  "outputDirectory": ".",
  "public": true,
  "cleanUrls": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
